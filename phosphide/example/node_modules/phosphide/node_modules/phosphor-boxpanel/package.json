{
  "name": "phosphor-boxpanel",
  "version": "1.0.0-beta",
  "description": "A Phosphor layout panel which arranges its children into a single row or column.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "phosphor-arrays": "^1.0.5",
    "phosphor-boxengine": "^1.0.1",
    "phosphor-domutil": "^1.0.0",
    "phosphor-messaging": "^1.0.5",
    "phosphor-properties": "^2.0.0",
    "phosphor-widget": "^1.0.0-beta"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "browserify-css": "^0.6.1",
    "browserify-istanbul": "^0.2.1",
    "coveralls": "^2.11.4",
    "expect.js": "^0.3.1",
    "glob-copy": "^0.1.0",
    "karma": "^0.13.9",
    "karma-browserify": "^4.3.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
